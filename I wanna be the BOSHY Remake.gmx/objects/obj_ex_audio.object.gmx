        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Handle fade in and out

if (not ex_audio_exists(_sound_name)) {
    instance_destroy();
}

_time += 1;

if (_easing == -1) {
    _value = abs(lerp(_value_start, _value_end, _time / _duration));
} else {
    _value = lerp(_value_start, _value_end, script_execute(_easing, _time, 0, clamp(_time * _duration, 0, 1), _duration));
}

switch (_mode) {

    case "volume":

        if (audio_system() == audio_new_system) {
            audio_emitter_gain(_sound_emitter, clamp(_value, 0, 1));
        } else {
            sound_volume(_sound, clamp(_value, 0, 1));
        }
        if (ds_exists(_list, ds_type_grid)) {
            ds_grid_set(_list, 5, _sound_y, _value);
        }
        
        break;

    case "pitch":
        
        if (audio_system() == audio_new_system) {
            audio_emitter_pitch(_sound_emitter, clamp(_value, 0, 5));
        }
        if (ds_exists(_list, ds_type_grid)) {
            ds_grid_set(_list, 6, _sound_y, _value);
        }
        
        break;
    
}

if (_time &gt;= _duration) {
    instance_destroy();
}

</string>
